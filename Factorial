import java.util.*;
public class Factorial
{
    public static void main(String[] args)
    {
        int i,n,fact=1;
        Scanner in = new Scanner(System.in);
        n=in.nextInt();
        for(i=n;i>0;i--)
        {
            fact = fact*i;
        }
        System.out.println(n+" "+fact);
    }
}
