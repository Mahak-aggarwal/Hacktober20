import java.util.*;
public class ReverseNumber
{
    public static void main(String args[])
    {
        int d,r=0,m;
        Scanner in = new Scanner(System.in);
        int n=in.nextInt();
        m=n;
        while(n>0)
        {
            d=n%10;
            r=r*10+d;
            n=n/10;
        }
        System.out.println(m+" "+r);
    }
}
